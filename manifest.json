{
    "name":"Data IQ",
    "description":"right now nothing",
    "version":"0.1.0",
    "manifest_version":3,
    "icons":{

    },
    "background":{
        "service_worker":"background.js"

    },
    "action":{
        "default_popup":"./popup.html",
        "default_icon":{

        }
    },
    "options_page":"./options.html",
    "permissions":[
        "storage", "activeTab", "scripting", "tabs", "declarativeNetRequest"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["./jquery.min.js","./interactor.js", "./dataiq_tracker.js"],
        "run_at":"document_start"
        
      }

    ],
    "declarative_net_request": {
        "rule_resources": [
          {
            "id": "ruleset_1",
            "enabled": true,
            "path": "rules.json"
          }
        ]
      }
}